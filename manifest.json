{
    "manifest_version": 2,
    "name": "Logging EZProxy Redirect",
    "description": "Quickly reload pages through your library's EZProxy.",
    "version": "13",
    "background": {
        "scripts": ["background.js"]
    },
    "options_page": "options.html",
    "permissions": [
        "contextMenus",
        "http://ezproxy-db.appspot.com/proxies.json"
    ],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Reload through EZProxy"
    }
}
